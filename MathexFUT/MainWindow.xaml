<Window x:Class="MathexFUT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
	    Style="{StaticResource w}" WindowStartupLocation="CenterScreen">
	<Grid>
		<Button Content="Vis Målsætninger" Style="{StaticResource btn}" Width="300" Click="OpenClick"/>
		<Button Content="EXIT" Style="{StaticResource btn}" Click="ExitClick"
				HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
	</Grid>
</Window>
